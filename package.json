    "eslint": "^8.57.1",
    "esno": "^0.16.3",
    "pnpm": "^8.15.9",
    "rimraf": "^4.4.1",
    "tsup": "^6.7.0",
    "typescript": "^5.6.2",
    "vitest": "^0.29.8",
    "vsce": "^2.15.0"
  }
